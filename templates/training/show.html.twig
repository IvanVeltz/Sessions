{% extends 'base.html.twig' %}

{% block title %}Détail d'un training {% endblock %}

{% block body %}
    <h1>Détail du training {{training.name}}</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Sessions</th>
            </tr>
        </thead>
        <tbody>
            {% for session in training.sessions %}
                <tr>
                    <td>{{session.name}}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
    

{% endblock %}